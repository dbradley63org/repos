@model IEnumerable<cycles.ef.tservice>
@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
	@Html.ActionLink("Create New", "Create")
</p>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.srvc_na)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.crt_dt)
		</th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.srvc_na)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.crt_dt)
			</td>
			<td>
				@Html.ActionLink("Edit", "Edit", new { id = item.srvc_id }) |
				@Html.ActionLink("Details", "Details", new { id = item.srvc_id }) |
				@Html.ActionLink("Delete", "Delete", new { id = item.srvc_id })
			</td>
		</tr>
	}

</table>
